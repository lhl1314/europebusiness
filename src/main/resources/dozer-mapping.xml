        <field>
            <a>rolePermissionsById</a>
            <b>rolePermissions</b>
        </field>
    </mapping>

    <mapping date-format="yyyy-MM-dd">
        <class-a>com.ouqicha.europebusiness.bean.entity.RolePermissionEntity</class-a>
        <class-b>com.ouqicha.europebusiness.bean.vo.RolePermissionVO</class-b>
        <field>
            <a>permissionByPermissionId</a>
            <b>permission</b>
        </field>
    </mapping>

</mappings>
