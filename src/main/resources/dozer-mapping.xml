<?xml version="1.0" encoding="UTF-8"?>
<mappings xmlns="http://dozer.sourceforge.net" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://dozer.sourceforge.net
          http://dozer.sourceforge.net/schema/beanmapping.xsd">
    <mapping date-format="yyyy-MM-dd">
        <class-a>com.ouqicha.europebusiness.bean.entity.AccountEntity</class-a>
        <class-b>com.ouqicha.europebusiness.bean.vo.AccountVO</class-b>
        <!--<field>-->
            <!--<a>personByPersonId</a>-->
            <!--<b>person</b>-->
        <!--</field>-->
        <!--<field>-->
            <!--<a>companyByCompanyId</a>-->
            <!--<b>company</b>-->
        <!--</field>-->

        <field-exclude>
            <a>personByPersonId</a>
            <b>person</b>
        </field-exclude>
        <field-exclude>
            <a>companyByCompanyId</a>
            <b>company</b>
        </field-exclude>

        <field>
            <a>accountRolesById</a>
            <b>accountRoles</b>
        </field>
        <field>
            <a>loginInfoEntity.type</a>
            <b>deviceType</b>
        </field>
        <field>
            <a>loginInfoEntity.deviceInfo</a>
            <b>deviceInfo</b>
        </field>
        <field>
            <a>loginInfoEntity.lastLoginTime</a>
            <b>lastLoginTime</b>
        </field>
    </mapping>
    <mapping date-format="yyyy-MM-dd">
        <class-a>com.ouqicha.europebusiness.bean.entity.CompanyEntity</class-a>
        <class-b>com.ouqicha.europebusiness.bean.vo.CompanyVO</class-b>
    </mapping>
    <mapping date-format="yyyy-MM-dd">
        <class-a>com.ouqicha.europebusiness.bean.entity.PersonEntity</class-a>
        <class-b>com.ouqicha.europebusiness.bean.vo.PersonVO</class-b>
        <field>
            <a>companyByCompanyId</a>
            <b>company</b>
        </field>

    </mapping>
    <mapping date-format="yyyy-MM-dd">
        <class-a>com.ouqicha.europebusiness.bean.entity.AccountRoleEntity</class-a>
        <class-b>com.ouqicha.europebusiness.bean.vo.AccountRoleVO</class-b>
        <field>
            <a>roleByRoleId</a>
            <b>role</b>
        </field>

    </mapping>
    <mapping date-format="yyyy-MM-dd">
        <class-a>com.ouqicha.europebusiness.bean.entity.RoleEntity</class-a>
